{
  "name": "gatsby-all-in",
  "description": "A all-in starter for Gatsby",
  "version": "0.1.0",
  "license": "MIT",
  "author": {
    "name": "Gherciu Gheorghe",
    "email": "gherciu553@gmail.com",
    "url": "https://gherciu.github.io/portfolio/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Gherciu/gatsby-all-in"
  },
  "bugs": {
    "url": "https://github.com/Gherciu/gatsby-all-in/issues"
  },
  "scripts": {
    "build": "gatsby build",
    "start": "gatsby develop",
    "serve": "gatsby serve",
    "lint:scripts": "eslint ./ --max-warnings=0",
    "fix:scripts": "eslint ./ --fix --max-warnings=0",
    "lint:styles": "stylelint \"./src/**/*.scss\" --syntax=scss",
    "fix:styles": "stylelint \"./src/**/*.scss\" --fix --syntax=scss",
    "clean:install": "rimraf node_modules && rimraf package-lock.json && npm i"
  },
  "lint-staged": {
    "*.(js|jsx)": [
      "npm run lint:scripts",
      "git add"
    ],
    "*.(css|scss)": [
      "npm run lint:styles",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "@ant-design/icons": "^4.1.0",
    "antd": "^4.2.3",
    "bourbon": "^7.0.0",
    "classnames": "^2.2.6",
    "gatsby": "^2.21.33",
    "lodash": "^4.17.15",
    "moment": "^2.25.3",
    "npm-check-updates": "^6.0.1",
    "prop-types": "^15.7.2",
    "ramda": "^0.27.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.0.0",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "tailwindcss": "^1.4.6",
    "yup": "^0.28.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@svgr/webpack": "^5.4.0",
    "babel-eslint": "^10.1.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.0.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.2",
    "gatsby-plugin-alias-imports": "^1.0.5",
    "gatsby-plugin-less": "^3.2.1",
    "gatsby-plugin-manifest": "^2.4.3",
    "gatsby-plugin-playground": "^1.0.6",
    "gatsby-plugin-postcss": "^2.3.1",
    "gatsby-plugin-react-helmet": "^3.3.1",
    "gatsby-plugin-sass": "^2.3.1",
    "gatsby-plugin-svgr": "^2.0.2",
    "husky": "^4.2.5",
    "less": "^3.11.1",
    "lint-staged": "^10.2.2",
    "node-sass": "^4.14.1",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "stylelint": "^13.4.0",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-prettier": "^1.1.2",
    "stylelint-scss": "^3.17.2"
  }
}
